{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": "airbnb-base",
  "rules": {
    "no-console": "warn",
    "max-len": ["error", { "code": 100 }],
    "consistent-return": "warn",
    "max-lines": "off",
    "max-params": "off",
    "linebreak-style": "off",
    "default-param-last": "off",
    "no-shadow": "off",
    "no-unused-vars": [
      "error",
      {
        "args": "none",
        "vars": "local",
        "varsIgnorePattern": "data"
      }
    ]
  }
}